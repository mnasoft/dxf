    ' Use \"\" to use the drawing name as the file name.
    Dim plotFileName As String
    plotFileName = \"MyPlot\"
    
    Dim result As Boolean
        
    result = ThisDrawing.Plot.PlotToFile(plotFileName)
    
End Sub
